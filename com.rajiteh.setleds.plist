<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>com.rajiteh.setleds</string>
	<key>ProgramArguments</key>
	<array>
		<string>/usr/bin/setleds</string>
		<string>monitor</string>
	</array>
	<key>RunAtLoad</key>
	<true/>
	<key>StandardOutPath</key>
	<string>/tmp/setleds.stdout</string>
	<key>StandardErrorPath</key>
	<string>/tmp/setleds.stderr</string>
	<key>KeepAlive</key>
	<true/>
</dict>
</plist>
